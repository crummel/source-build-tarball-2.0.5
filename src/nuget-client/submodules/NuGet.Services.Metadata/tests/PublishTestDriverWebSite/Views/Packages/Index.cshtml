@model PublishTestDriverWebSite.Models.PackagesModel
@{
    ViewBag.Title = "Packages";
}

<h1>Packages Page</h1>

<table style="width: 60%">
    <tr >
        <th>Domain</th>
        <th>Id</th>
        <th>Version</th>
        <th>Visibility</th>
    </tr>
    @foreach (var package in Model.Packages)
    {
        <tr>
            <td>@package.Domain</td>
            <td>@package.Id</td>
            <td>@package.Version</td>
            <td>@package.Visibility</td>
        </tr>
    }
</table>
